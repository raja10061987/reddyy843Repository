<apex:page controller="BlogsContinutionController">
<apex:form >
    <apex:pageBlock title="HttpCallout Async call" id="mainBlock">
    <apex:commandButton value="send Request" action="{!sendAsyncCallOutRequest}" reRender="mainBlock"></apex:commandButton>

    <apex:pageBlock title="{!bWrapper.author + '  Blogs List'}">
    <apex:pageBlockTable value="{!bWrapper.blogs}" var="blog">
    <apex:column >
     <apex:facet name="header">Id</apex:facet>
     {!blog.id}
    </apex:column>
    <apex:column >
    <apex:facet name="header">title</apex:facet>
    {!blog.title}
    </apex:column>
    <apex:column >
    <apex:facet name="header">url</apex:facet>
    <apex:outputLink id="url" value="{!blog.url}" target="_blank">{!blog.url}</apex:outputLink>
    </apex:column>
    </apex:pageBlockTable>

    </apex:pageBlock>

   <apex:pageBlock title="{!appWrapper.author + '  appList'}">
   <apex:pageBlockTable value="{!appWrapper.apps}" var="app">
     <apex:column >
      <apex:facet name="header">Id</apex:facet>
      {!app.id}
     </apex:column>
     <apex:column >
     <apex:facet name="header">title</apex:facet>
     {!app.title}
     </apex:column>
     <apex:column >
     <apex:facet name="header">URL</apex:facet>
     <apex:outputLink id="url" value="{!app.url}" target="_blank">{!app.url}</apex:outputLink>
     </apex:column>
   </apex:pageBlockTable>
   </apex:pageBlock>

   <apex:pageBlock title="{!sWrapper.author + '  sessions'}">
     <apex:pageBlockTable value="{!sWrapper.sessions}" var="session">
         <apex:column >
           <apex:facet name="header">Id</apex:facet>
            {!session.id}
         </apex:column>
         <apex:column >
            <apex:facet name="header">title</apex:facet>
            {!session.title}
         </apex:column>
         <apex:column >
            <apex:facet name="header">URL</apex:facet>
            <apex:outputLink value="{!session.url}" target="_blank">{!session.url}</apex:outputLink>
         </apex:column>
         
     </apex:pageBlockTable>
   </apex:pageBlock>




    </apex:pageBlock>
</apex:form>
</apex:page>