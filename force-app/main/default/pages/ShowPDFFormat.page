<apex:page controller="RetrieveContacts" readOnly="true" contentType="application/vnd.ms-excel">
  
   <div style="background:url('{!$Resource.image}') no-repeat center center;height:800px;font-family: Arial, Helvetica, sans-serif">
      <h2>Contacts: ({!contList.size})</h2>
      <table style="border-collapse: collapse;width:100%">
         <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone</th>
         </tr>
         <apex:repeat value="{!contList}" var="contObj">
            <tr style="padding: 8px;text-align: left;border-bottom: 1px solid #ddd;">
               <td style="padding: 8px;text-align: left;border-bottom: 1px solid #ddd;">{!contObj.FirstName}</td>
               <td style="padding: 8px;text-align: left;border-bottom: 1px solid #ddd;">{!contObj.LastName}</td>
               <td style="padding: 8px;text-align: left;border-bottom: 1px solid #ddd;">{!contObj.Phone}</td>
            </tr>
         </apex:repeat>
      </table>
   </div>
</apex:page>