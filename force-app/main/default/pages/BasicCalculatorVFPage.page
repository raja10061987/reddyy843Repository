<apex:page setup="false" sidebar="false" showHeader="true" tabStyle="Flight__c">
<apex:sectionHeader title="Basic caluculator Example" subtitle="Calculator" />
<apex:form >
<apex:pageBlock title="pageblock">
<apex:pageBlockButtons >
<apex:commandButton value="save" action="{!Save}"/>
<apex:commandButton value="Cancel" action="{!cancel}"/>
<apex:commandButton value="save&New" action="{!save}"/>
<apex:commandLink value="first" action="{!first}"/> &nbsp;
<apex:commandLink value="last" action="{!last}"/>
</apex:pageBlockButtons>
<apex:pageBlockSection title="Basic calculator">
<apex:inputText label="Enter First name" html-placeholder="First Name" />
<apex:inputText label="Enter Last Name" html-placeholder="LastName"/>
</apex:pageBlockSection>
<apex:pageblockSection title="Additional info">

</apex:pageblockSection>
</apex:pageBlock>
 
</apex:form>
</apex:page>