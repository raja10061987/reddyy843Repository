<apex:page standardController="Account" extensions="FetchAccountRecords">
  <apex:form >
  <apex:pageBlock title="Account Edit">
  <apex:pageBlockSection title="Account Information" collapsible="true" columns="3">
  <apex:inputField value="{!Account.Name}"/>
  <apex:inputField value="{!Account.Rating}"/>
  <apex:inputField value="{!Account.ParentId}"/>
  <apex:inputField value="{!Account.Fax}"/>
  </apex:pageBlockSection>
  
  <apex:pageBlockSection title="Address Information">
  <h1> there is no address page</h1>
  </apex:pageBlockSection>
  
  <apex:pageBlockSection title="Additional information">
  
  </apex:pageBlockSection>
  
  <apex:pageBlockSection title="Description Information" collapsible="false" columns="2">
  <apex:inputField value="{!Account.CustomerPriority__c}"/>
  <apex:inputField value="{!Account.SLA__c}"/>
  <apex:inputField value="{!Account.SLAExpirationDate__c}"/>
  <apex:inputField value="{!Account.Active__c}"></apex:inputField>
  <apex:inputField value="{!Account.UpsellOpportunity__c}"/>
  </apex:pageBlockSection>
  <apex:pageBlockButtons location="both">
  <apex:commandButton value="save" action="{!save}" />
  <apex:commandButton value="cancel" action="{!cancel}"/>
  </apex:pageBlockButtons>
  </apex:pageBlock>
  
  <apex:pageBlock title="Account Records">
  <apex:pageBlockTable value="{!actList}" var="actObj">
  <apex:column value="{!actObj.Name}"/>
  <apex:column value="{!actObj.Phone}"/>
  <apex:column value="{!actObj.Rating}"/>
  <apex:column value="{!actObj.Budget__c}"/>
  </apex:pageBlockTable>
  </apex:pageBlock>
  </apex:form>
</apex:page>